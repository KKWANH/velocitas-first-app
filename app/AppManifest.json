{
    "manifestVersion": "v3",
    "name": "SampleApp",
    "interfaces": [
        {
            "type": "vehicle-signal-interface",
            "config": {
                "src": "https://github.com/COVESA/vehicle_signal_specification/releases/download/v3.0/vss_rel_3.0.json",
                "datapoints": {
                    "required": [
                        {
                            "path": "Vehicle.Speed",
                            "access": "read"
                        },
                        {
                            "path": "Vehicle.AverageSpeed",
                            "access": "read"
                        },
                        {
                            "path": "Vehicle.IsMoving",
                            "access": "read"
                        }
                    ]
                }
            }
        },
        {
            "type": "pubsub",
            "config": {
                "reads": [
                    "sampleapp/getSpeed",
                    "sampleapp/getAverageSpeed",
                    "sampleapp/getIsMoving"
                ],
                "writes": [
                    "sampleapp/getSpeed/response",
                    "sampleapp/getAverageSpeed/response",
                    "sampleapp/getIsMoving/response",
                    "sampleapp/currentSpeed",
                    "sampleapp/currentAverageSpeed",
                    "sampleapp/currentIsMoving"
                ]
            }
        }
    ]
}
